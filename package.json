{
  "name": "apiline",
  "version": "1.0.0",
  "description": "A collection of API packages for various services",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "start": "pnpm -r start",
    "start:dev": "pnpm -r start:dev",
    "start:prod": "pnpm -r start:prod",
    "dev": "pnpm -r start:dev",
    "test": "pnpm -r test",
    "test:watch": "pnpm -r test:watch",
    "test:cov": "pnpm -r test:cov",
    "test:e2e": "pnpm -r test:e2e",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "format": "pnpm -r format",
    "clean": "pnpm -r clean",
    "install:all": "pnpm install",
    "update:all": "pnpm update -r"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navtej927/apiline.git"
  },
  "keywords": ["api", "microservices", "pnpm", "workspace"],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/navtej927/apiline/issues"
  },
  "homepage": "https://github.com/navtej927/apiline#readme",
  "devDependencies": {
    "typescript": "^5.2.0",
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
